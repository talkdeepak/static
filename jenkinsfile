pipeline {
     agent any
     stages {
         stage('Upload to AWS') {
             steps {
                 withAWS(region:'us-west-2',credentials:'7H49/d8D0gtbd7Ha+1BmNtkFt2aqVTozbiKDSqxL') {sh 'echo "Uploading content with AWS creds"'
                 s3Upload(pathStyleAccessEnabled: true, payloadSigningEnabled: true, file:'index.html', bucket:'jenkinsudacity')
             }
         }
     }
}
